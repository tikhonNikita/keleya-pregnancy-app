{
  "name": "keleya-pregnancy-app",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "npx expo start",
    "android": "npx expo run:android",
    "ios": "npx expo run:ios",
    "web": "npx expo start --web",
    "prebuild": "npx expo prebuild --clean",
    "ts:check": "tsc --noEmit",
    "eas-build-pre-install": "./eas-hooks/eas-build-pre-install.sh",
    "eas-build-on-success": "./eas-hooks/eas-build-on-success.sh",
    "e2e:android": "npx expo prebuild --clean && detox build -c android.release && detox test -c android.local",
    "e2e:ios": "npx expo prebuild --clean && detox build -c ios.release && detox test -c ios.local",
    "lint": "eslint ."
  },
  "dependencies": {
    "@react-native-community/datetimepicker": "7.2.0",
    "@react-native-picker/picker": "2.4.10",
    "@react-navigation/native": "^6.1.8",
    "@react-navigation/native-stack": "^6.9.14",
    "expo": "~49.0.13",
    "expo-checkbox": "~2.4.0",
    "expo-linear-gradient": "~12.3.0",
    "expo-localization": "~14.3.0",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "i18n-js": "^4.3.2",
    "make-plural": "^7.3.0",
    "react": "18.2.0",
    "react-hook-form": "^7.47.0",
    "react-i18next": "^13.2.2",
    "react-native": "0.72.5",
    "react-native-modal-datetime-picker": "^17.1.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@config-plugins/detox": "^6.0.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@types/detox": "^18.1.0",
    "@types/jest": "^29.5.5",
    "@types/react": "~18.2.14",
    "detox": "^20.12.2",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "looks-same": "^8.2.3",
    "prettier": "^2.4.1",
    "typescript": "^5.1.3"
  },
  "private": true
}
